    "doctrine/orm": "^2.17 || ^3.0",
    "doctrine/common": "^3.5",
    "doctrine/doctrine-bundle": "^2.13",
    "symfony/framework-bundle": "^6.4",
    "symfony/property-access": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/bundle-dependency": "0.0.*",
