{
  "name": "tourze/doctrine-snowflake-bundle",
  "description": "为 Doctrine 实体提供雪花ID自动生成功能",
  "license": "MIT",
  "require": {
    "ext-reflection": "*",
    "doctrine/common": "^3.5",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^4.1",
    "monolog/monolog": "^3.1",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/expression-language": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/lock": "^7.3",
    "symfony/messenger": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/string": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/doctrine-entity-checker-bundle": "1.0.*",
    "tourze/easy-admin-menu-bundle": "0.1.*",
    "tourze/symfony-dependency-service-loader": "1.*",
    "tourze/symfony-snowflake-bundle": "0.0.*"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^4.0",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/phpunit-bridge": "^7.3",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "suggest": {
    "ext-redis": "*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\DoctrineSnowflakeBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\DoctrineSnowflakeBundle\\Tests\\": "tests"
    }
  }
